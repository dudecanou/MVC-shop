@model IEnumerable<MVC_miSessionHA.Models.Skateboard>
@{
    ViewBag.Title = "Consultation";
}


<h2>@ViewBag.Title</h2>
<br />

<h3>Skateboards de la marque <font color="purple" size="6">@ViewBag.nomMarque</font></h3>
    <br />
    <hr />

    <table class="table">
        <tr>


            <th>
                @Html.DisplayNameFor(model => model.Style)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Image)
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>




                <td>
                    @Html.DisplayFor(model => item.Style)
                </td>

                <td>
                    <img src="http://@Html.DisplayFor(modelItem => item.Image)" width="120px" />
                </td>


            </tr>
        }
    </table>
    <hr />


