@model IEnumerable<MVC_miSessionHA.Models.Accessoire>

@{
    ViewBag.Title = "Index";
}

<h2>Accessoires</h2>
<br />

<!--<p>
    @Html.ActionLink("Create New", "Create")
</p>-->
<table class="table" style="text-align:center">
    <tr>
        <th style="text-align:center">
            @Html.DisplayNameFor(model => model.Skateboard.Style)
        </th>
        <th style="text-align:center">
            @Html.DisplayNameFor(model => model.NomAcc)
        </th>
        <th style="text-align:center">
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th style="text-align:center">
            @Html.DisplayNameFor(model => model.Prix)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Skateboard.Style)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomAcc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prix)
        </td>
        <td>
          <!--  @Html.ActionLink("Edit", "Edit", new { id=item.AccessoireID }) |-->
            @Html.ActionLink("Details", "Details", new { id=item.AccessoireID }) 
           <!-- @Html.ActionLink("Delete", "Delete", new { id=item.AccessoireID }) -->
        </td>
    </tr>
}

</table>
